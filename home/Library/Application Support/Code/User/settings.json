{
    "vim.enableNeovim": true,
    "vim.disableAnnoyingNeovimMessage": true,
    // "editor.fontFamily": "OperatorMonoForPowerline-Book",
    "editor.fontFamily": "Fira Mono",
    "editor.fontSize": 14,
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "editor.lineNumbers": "relative",
    "editor.cursorBlinking": "solid",
    "vim.easymotion": true,
    "vim.hlsearch": true,
    // "workbench.welcome.enabled": false,
    "workbench.colorTheme": "Oceanic Next",
    // "typescript.check.tscVersion": false,
    "workbench.startupEditor": "newUntitledFile",
    "editor.tabSize": 2,
    "editor.wordWrap": "on",
    "window.zoomLevel": 0,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/.happypack": true,
        "**/.vscode": true,
        "tags": true,
        "dist": true
    },
    "workbench.statusBar.visible": true,
    "editor.scrollBeyondLastLine": false,
    "editor.fontLigatures": true,
    "files.insertFinalNewline": true,
    "workbench.activityBar.visible": false,
    "vim.leader": "<space>",
    "terminal.external.osxExec": "iTerm.app",
    "workbench.tips.enabled": false,
    "explorer.openEditors.visible": 0,
    "git.confirmSync": false,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "vue"
    ],
    "window.restoreWindows": "none",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.lineHeight": 0,
    "editor.formatOnType": true,
    "editor.formatOnPaste": true,
    "editor.renderWhitespace": "none",
    "editor.dragAndDrop": false,
    "editor.accessibilitySupport": "off",
    "diffEditor.renderSideBySide": false,
    "files.autoGuessEncoding": false,
    "files.trimTrailingWhitespace": true,
    "vetur.format.styleInitialIndent": true,
    "vetur.format.scriptInitialIndent": true,
    "git.enableCommitSigning": true,
    "window.nativeTabs": false,
    "AsciiDoc.html_generator": "asciidoctor -o-",
    "vim.statusBarColorControl": true,
    "vim.visualstar": true,
    "vim.foldfix": true,
    "vim.substituteGlobalFlag": true,
    "telemetry.enableTelemetry": false,
    "gitlens.advanced.telemetry.enabled": false,
    "code-runner.enableAppInsights": false,
    "rest-client.enableTelemetry": false,
    "gitlens.blame.line.enabled": false,
    "gitlens.blame.file.annotationType": "hover",
    "python.venvPath": "~/pyenv",
    "python.pythonPath": "~/.pyenv/shims/python",
    "[python]": {
        "editor.tabSize": 4
    },
    "[rust]": {
        "editor.tabSize": 4
    },
    "vim.neovimPath": "/usr/local/bin/nvim",
    "stylelint.enable": true,
    "css.validate": false,
    "scss.validate": false,
    "explorer.confirmDelete": false,
    "workbench.panel.location": "right",
    "editor.rulers": [
        80,
        120
    ],
    "files.encoding": "utf8",
    "files.associations": {
        "*.html": "html"
    },
    "vim.hlsearch": true,
    "vim.cmdLineInitialColon": true,
    "vim.sneak": true,
    "workbench.statusBar.feedback.visible": false,
    "vim.otherModesKeyBindingsNonRecursive": [
        {
            "before": ["j"],
            "after": ["g", "j"]
        },
        {
            "before": ["k"],
            "after": ["g", "k"]
        }
    ],
    "eslint.packageManager": "yarn",
    "extensions.ignoreRecommendations": true,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "telemetry.enableCrashReporter": false,
    "npm.packageManager": "yarn",
    "eslint.options": { "configFile": "/Users/mbischoff/.eslintrc.json" }
    // "editor.hover": false
}
